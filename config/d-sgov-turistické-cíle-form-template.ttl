d-sgov-turistické-cíle:form-template a owl:Ontology .
	
d-sgov-turistické-cíle:form-template skos:prefLabel "Turistický cíl - komplexní OFN".

d-sgov-turistické-cíle-pojem:turistický-cíl a ft:root-entity .

g-sgov-věci-pojem:název a ft:object-identifier-quality .

d-sgov-turistické-cíle-pojem:kouření-povoleno a ft:boolean .
d-sgov-turistické-cíle-pojem:veřejná-přístupnost a ft:boolean .
#d-sgov-turistické-cíle-pojem:kapacita a ft:number .


g-sgov-věci-pojem:aktualizováno   ft:ignore true .
g-sgov-věci-pojem:vytvořeno   ft:ignore true .
g-sgov-věci-pojem:zneplatněno   ft:ignore true .
 
v-sgov-pojem:subjekt-práva ft:ignore true .
v-sgov-pojem:má-platnost ft:ignore true .


# clovek, osoba
v-sgov-pojem:křesní-jméno ft:ignore true .
v-sgov-pojem:příjmení ft:ignore true .
<https://slovník.gov.cz/generický/lidé/pojem/celé-jméno> a ft:object-identifier-quality .


# vlastnik, provozovatel
